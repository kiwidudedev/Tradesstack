{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/lib/*": [
        "lib/*"
      ],
      "@/theme/*": [
        "theme/*"
      ],
      "@/types/*": [
        "types/*"
      ]
    }
  },
  "include": [
    "app",
    "components",
    "lib",
    "theme",
    "types",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
